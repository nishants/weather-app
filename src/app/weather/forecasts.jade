.form
  input(placehodler='Search a city')
  button(ng-click='') Search

table
  thead
    tr
      td City
      td 12 AM
      td 6 AM
      td 12 PM
      td 6 PM

  tbody
    tr(ng-repeat='city in forecasts.cities')
      td {{city.name}}
      td {{city.midnight}} C
      td {{city.noon}} C
      td {{city.evening}} C